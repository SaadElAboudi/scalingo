    function createSpaceElement() {
        let space = document.createElement("div");
        space.classList.add("bg-black", "outer-space", "h-screen", "w-screen", "z-51", "fixed", "opacity-0", "transition-opacity", "duration-500", "ease-in-out");
        document.body.insertBefore(space, document.body.firstChild);

        let stars = document.createElement("div");
        stars.classList.add("stars");
        space.appendChild(stars);

        return space;
    }

    function launchRocket() {
        let rocket = document.getElementsByClassName('rocket')[0];

        let ejections = rocket.querySelectorAll(".ejections");
        for (let i = 0; i < ejections.length; i++) {
            let ejection = ejections[i];
            ejection.classList.add("animate-pulse")
        }

        let space = createSpaceElement();

        rocket.classList.add("transition", "duration-1000");

        setTimeout(function () {
            space.classList.remove("opacity-0");
            space.classList.add("opacity-100");
            rocket.classList.add("z-52");
        }, 1000);

        setTimeout(function () {
            rocket.classList.add("bg-black", "text-white", "-rotate-45");
        }, 1500);

        setTimeout(function () {
            rocket.classList.remove("bg-black");
        }, 7000);

        setTimeout(function () {
            space.classList.remove("opacity-100");
            rocket.classList.remove("text-white", "-rotate-45");
            space.classList.add("opacity-0");
        }, 8000);

        setTimeout(function () {
            space.remove();
            rocket.classList.remove("z-52");
        }, 9000);
    }
